function VerificaIntervaloInteiroBranco(Texto:ShortString):Boolean;
var
  i, j: integer;
  sAux,sNumero1, sNumero2: ShortString;
  vNumeros: array of ShortString;
Begin
  result:=True;
  if (Trim(Texto) = '') or
     (Trim(Texto) = '-') then
  Begin
    Result := False;
    Exit;
  end
  else if (Pos('-',Texto) <= 0) and
          (Pos(';',Texto) <= 0) then
  Begin
    Result := False;
    Exit;
  end;
  if Pos('-',Texto) > 0 then
  Begin
    sNumero1 := Copy(Texto,1,Pos('-',Texto)-1);
    sNumero2 := Copy(Texto,Pos('-',Texto)+1,Length(Texto)-Pos('-',Texto));
    try
      StrToInt(sNumero1);
      StrToInt(sNumero2);
    except
      Result := False;
      Exit;
    end;
  end
  else //Intervalo separado por ";"
  Begin
    j := 0;
    sAux := '';
    for i:=1 to length(Texto) do
    Begin
      if Texto[i] = ';' then
      Begin
        SetLength(vNumeros,j+1);
        vNumeros[j] := sAux;
        sAux := '';
        inc(j);
      end
      else
        sAux := sAux + Texto[i];
    end;
    SetLength(vNumeros,j+1);
    vNumeros[j] := sAux;
    for i:=0 to j do
    Begin
      try
        StrToInt(vNumeros[i]);
      except
        Result := False;
        Exit;
      end;
    end;
  end;
End;

function DesmontaIntervalo(Intervalo:ShortString; CampoInicial:ShortString='('; Separador:ShortString=','; CampoFinal:ShortString=')'): ShortString;
var
  i, j: integer;
  sAux, sNumero1, sNumero2, Expressao: ShortString;
  vNumeros: array of ShortString;
Begin
  Result := '';
  if not VerificaIntervaloInteiroBranco(Intervalo) then
    Exit;
  if Pos('-',Intervalo) > 0 then
  Begin
    sNumero1 := Copy(Intervalo,1,Pos('-',Intervalo)-1);
    sNumero2 := Copy(Intervalo,Pos('-',Intervalo)+1,Length(Intervalo)-Pos('-',Intervalo));
    if Length(Separador) > 1 then
      Result := CampoInicial   + Separador      + '>=' + sNumero1 + ' AND ' + Separador       + '<=' + sNumero2 + CampoFinal
      //Ex:  ' AND ( '         + T1.PRD_CODIGO' + '>=' + 122      + ' AND ' + 'T1.PRD_CODIGO' + '<=' + 2333     + ')'
    else (*       Ex: ","  ";"         *)
      Result := CampoInicial   + sNumero1 + Separador + sNumero2 + CampoFinal
      //Ex:  '('               + 122      + ','       + 2333     + ')'
  end
  else
  Begin
    j := 0;
    sAux := '';
    for i:=1 to length(Intervalo) do
    Begin
      if Intervalo[i] = ';' then
      Begin
        SetLength(vNumeros,j+1);
        vNumeros[j] := sAux;
        sAux := '';
        inc(j);
      end
      else
        sAux := sAux + Intervalo[i];
    end;
    SetLength(vNumeros,j+1);
    vNumeros[j] := sAux;
    Expressao := '';
    for i:=0 to j do
    Begin
      if i>0 then
        Expressao := Expressao + ' OR ';
      Expressao := Expressao + Separador + '=';
      Expressao := Expressao + vNumeros[i];
    end;
    Result := CampoInicial + Expressao + CampoFinal;
  end;
end;

function VerificaBase(PGrau: Double):Double; stdcall; Export;
Begin
  Result := 0;
  if      (PGrau >=  -7.25) and (PGrau <= -12.00) then
    Result := 0.5
  else if (PGrau >=  -4.75) and (PGrau <=  -7.00) then
    Result := 2.5
  else if (PGrau >=  -2.00) and (PGrau <=  -4.50) then
    Result := 4
  else if (PGrau >=  -1.75) and (PGrau <=   4.50) then
    Result := 6
  else if (PGrau >=   4.75) and (PGrau <=   7.75) then
    Result := 8
  else if (PGrau >=   8.00) and (PGrau <=   9.75) then
    Result := 10
  else if (PGrau >=  10.00) and (PGrau <=  11.75) then
    Result := 12;
end;

function ApagarArquivos(PPasta, PNomeArquivo: ShortString; PExcecaoSemCoringas: ShortString=''; PExcecaoSemCoringas2: ShortString=''):Boolean; stdcall; Export;
var
  sr: TSearchRec;
  i: integer;
  ListaArquivos: TStringList;
begin
  Result := True;
  ListaArquivos := TStringList.Create;
  try
    if FindFirst(PPasta+'\'+PNomeArquivo, 0, sr) = 0 then
    begin
      repeat
        ListaArquivos.Add(PPasta+'\'+sr.Name);
      until FindNext(sr) <> 0;
      FindClose(sr);
    end;
    if ListaArquivos.Count > 0 then
    Begin
      for i:=0 to ListaArquivos.Count-1 do
      Begin
        if (UpperCase(ListaArquivos[i]) <> UpperCase(PPasta+'\'+PExcecaoSemCoringas)) and
           (UpperCase(ListaArquivos[i]) <> UpperCase(PPasta+'\'+PExcecaoSemCoringas2)) then
        Begin
          if not DeleteFile(ListaArquivos[i]) then
          Begin
            Result := False;
            ShowMessage('Não foi possível apagar o arquivo ('+ListaArquivos[i]+' !');
            Exit;
          end;
        end;
      end;
    end
    else //Não encontrou o arquivo
      Result := False;
  finally
    ListaArquivos.Free;
  end;
end;

function AbreviarNome(Nome: ShortString): ShortString; stdcall; Export;
var
  Nomes: array[1..20] of string;
  i, TotalNomes: Integer;
begin
  Nome := Trim(Nome);
  Result := Nome;
  {Insere um espaço para garantir que todas as letras sejam testadas}
  Nome := Nome + #32;
  {Pega a posição do primeiro espaço}
  i := Pos(#32, Nome);
  if i > 0 then
  begin
    TotalNomes := 0;
    {Separa todos os nomes}
    while i > 0 do
    begin
      Inc(TotalNomes);
      Nomes[TotalNomes] := Copy(Nome, 1, i - 1);
      Delete(Nome, 1, i);
      i := Pos(#32, Nome);
    end;
    if TotalNomes > 2 then
    begin
      {Abreviar a partir do segundo nome, exceto o último.}
      for i := 2 to TotalNomes - 1 do
      begin
        {Contém mais de 3 letras? (ignorar de, da, das, do, dos, etc.)}
        if Length(Nomes[i]) > 3 then
          {Pega apenas a primeira letra do nome e coloca um ponto após.}
          Nomes[i] := Nomes[i][1] + '.';
      end;
      Result := '';
      for i := 1 to TotalNomes do
        Result := Result + Trim(Nomes[i]) + #32;
      Result := Trim(Result);
    end;
  end;
end;

